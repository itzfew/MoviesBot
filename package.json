{
  "name": "eduhub",
  "version": "1.1.2",
  "description": "Telegram Bot EduHub - Google Sheets + Telegraf",
  "main": "src/index.ts",
  "author": "itzfew (https://t.me/itzfew)",
  "homepage": "https://github.com/itzfew/eduhub-bot",
  "dependencies": {
  "axios": "^1.7.2",
  "cashfree-pg": "^4.2.6",
  "dotenv-cli": "^7.4.2",
  "google-spreadsheet": "^3.3.0",
  "node-fetch": "^3.3.2",
  "telegraf": "^4.16.3",
  "undici": "^6.19.8",
  "@vercel/node": "^3.2.24"
},
"devDependencies": {
  "@types/node": "^22.8.6",
  "@types/undici": "^6.18.2",
  "@types/vercel__node": "^2.1.2",
  "typescript": "^5.6.3",
  "ts-node": "^10.9.2",
  "nodemon": "^3.1.7"
},
  "scripts": {
    "dev": "DEBUG=bot* dotenv -e .env -- ts-node src/index.ts",
    "dev:watch": "DEBUG=bot* dotenv -e .env -- nodemon -e ts -x ts-node src/index.ts",
    "build": "ncc build src/index.ts -o public -m",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "tsc --noemit",
    "start": "node public/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
