{
  "builds": [
    {
      "src": "public/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.js"
    }
  ],
  "functions": {
    "public/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}
