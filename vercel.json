{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "functions": {
    "src/index.ts": {
      "memory": 2048,
      "maxDuration": 60
    }
  },
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}
